#Query che seleziona tutte le officine recensite e il voto corrispondente per ogni recensione ricevuta
select
	o.nome,
	v.voto
from
	officina o,
	recensioni r,
	voto v
where
	o.id = r.id_officina
	and r.id_voto = v.id
	and
v.voto notnull

